MAX_SIZE = 1<<26
TAB = 2

generate = (n, tabSize) ->
 T = ""
 for i in [0...tabSize]
  T += " "
 size = 8
 sum = 0
 l = 0
 for i in [0...n]
  if size isnt MAX_SIZE
   size = size << 1
  sum += size
  str = """
   if p < #{sum}
    findRes[0] = #{i}
    findRes[1] = p - #{l}
    return
  """
  arr = str.split '\n'
  for v, i in arr
   arr[i] = "#{T}#{v}"

  console.log arr.join '\n'
  l = sum

generate 40, TAB

